

@page "/manage-answer"
    <h3>Manage Answer</h3>
    <button class="btn btn-primary my-md-4">Add Answer</button>
<div class="card mb-4">
    <div class="card-body">
        <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" />
        </div>
        <div class="form-group">
            <label>Sequence</label>
            <input class="form-control" type="number" />
        </div>
        <div class="custom-control custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="customCheck1">
            <label class="custom-control-label" for="customCheck1">IsCorrect</label>
        </div>

    </div>
</div>
